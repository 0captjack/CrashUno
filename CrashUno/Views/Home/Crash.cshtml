@{
    ViewData["Title"] = "Crashes";
}

@model CrashViewModel

@*<h2>@ViewBag.Calc</h2>*@
<h1>@ViewData["Title"]</h1>


    @foreach (Crash c in Model.Crash)
    {

        <ul>
            <li>Crash ID: @c.crash_id</li>
            <li>Timestamp: @c.crash_datetime </li>
            <li>RouteMarker: @c.route</li>
        </ul>

        <br />
    }

<div page-blah="@Model.PageInfo" page-action="Crash"></div>

@*<a asp-action="Crash" asp-route-pageNum="1">1</a>*@


